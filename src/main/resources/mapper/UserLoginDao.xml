<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace ="com.jung.onlinechat.dao.UserLoginDao">
    <select id="userSelect" resultType="com.jung.onlinechat.entity.User">
        SELECT * FROM user
        WHERE name=#{username}
    </select>
</mapper>